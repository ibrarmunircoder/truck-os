    "@roq/class-validator": "^0.1.0",
    "@roq/nestjs-dataloader": "^0.0.1",
    "apollo-server-express": "^3.8.1",
    "aws-sdk": "^2.1247.0",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.0",
    "cache-manager": "^4.0.1",
    "class-transformer": "^0.3.1",
    "commander": "^8.3.0",
    "crypto-js": "^4.1.1",
    "cryptr": "^6.0.3",
