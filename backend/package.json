    "isomorphic-fetch": "^3.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "nest-winston": "^1.6.2",
    "nestjs-cls": "^1.6.2",
    "nestjs-console": "^7.0.1",
    "nestjs-s3": "^1.0.1",
    "newrelic": "^8.12.0",
    "passport": "^0.4.1",
    "passport-headerapikey": "^1.2.2",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
