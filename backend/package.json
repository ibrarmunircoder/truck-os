{
  "name": "@truckos/backend",
  "version": "0.0.1",
  "description": "",
  "author": "truckOS GmbH",
  "graphdoc": {
    "endpoint": "https://roq-snapshot-space-ytqhysh7ka-ew.a.run.app/graphql",
    "output": "./doc/schema"
  },
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "nest build",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "console": "node dist/src/console.js",
    "createSchema": "node_modules/.bin/ts-node -r tsconfig-paths/register src/create-schema.ts ",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generateDocs": "node_modules/.bin/spectaql graphqlDocs/config.yml -t ./graphqlDocs/docs",
    "import:dev": "npm run console:dev import-entities",
    "import:initial": "npm run console:dev import-entities -- --initial",
    "import": "node dist/src/console.js import-entities",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "schema:local:sync": "npm run typeorm:local -- schema:sync",
    "migration:local:config": "ts-node -r tsconfig-paths/register src/migration local",
    "migration:local:generate": "npm run typeorm:local -- migration:generate -n ",
    "migration:local:create": "npm run typeorm:local -- migration:create -n ",
    "migration:local:revert": "npm run typeorm:local -- migration:revert",
    "migration:local:run": "npm run typeorm:local -- migration:run",
    "migration:local:show": "npm run typeorm:local -- migration:show",
    "prebuild": "rimraf dist",
    "pretest:e2e": "docker-compose up -d test-db",
    "run:jest": "node_modules/.bin/ts-node -r tsconfig-paths/register ./test/pre-tests && node_modules/.bin/jest --ci --testTimeout=20000 --runInBand --coverage --passWithNoTests --detectOpenHandles --forceExit --config ./jest.config.js",
    "sniff": "eslint src/**/*.ts --fix -f json -o ../backend-lint-report.json",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/src/main.js",
    "start": "nest start",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "test": "ts-node -r tsconfig-paths/register test/pre-tests.ts && jest --detectOpenHandles --forceExit",
    "typeorm:local": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "script:decryptWalbingKey": "ts-node scripts/decrypt-key.script.ts"
  },
  "postinstall": "npm run prestart:prod",
  "dependencies": {
    "@apollo/client": "^3.6.5",
